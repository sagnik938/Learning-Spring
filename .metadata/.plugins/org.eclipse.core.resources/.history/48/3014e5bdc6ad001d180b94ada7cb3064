package com.spring.DemoMvc.controller;

import org.springframework.context.ApplicationContext;
import org.springframework.context.support.ClassPathXmlApplicationContext;

import com.spring.DemoMvc.dao.EmployeeDao;

public class EmployeeController {
	public static void main(String[] args) {
		ApplicationContext context = new ClassPathXmlApplicationContext("./WEB-INF/springservlet.xml");
		EmployeeDao eD = context.getBean("employeeDao",EmployeeDao.class);
		System.out.println(eD);
	}
}
